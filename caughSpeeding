public int caughtSpeeding(int speed, boolean isBirthday) {
  if ((isBirthday) && (speed >= 0 && speed <= 65 )){
    return 0;
  }else if ((isBirthday) &&  (speed >= 66 && speed <= 85)){
    return 1;
  }else if ((isBirthday) && (speed >= 86)){
    return 2;
  }else if (speed >= 0 && speed <= 60 ){
    return 0;
  }else if (speed >= 61 && speed <= 80){
    return 1;
  }else if (speed >= 81){
    return 2;
  }
  return 3; 
}
